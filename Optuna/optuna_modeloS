   "source": [
    "import optuna\n",
    "import tensorflow as tf\n",
    "from keras import regularizers, layers, models, optimizers\n",
    "from tensorflow.keras import Input\n",
    "from tensorflow.keras.utils import image_dataset_from_directory\n",
    "\n",
    "train_dir = 'dataset_balanceado_final/train'\n",
    "validation_dir = 'dataset_balanceado_final/validation'\n",
    "test_dir = 'dataset_balanceado_final/test'\n",
    "\n",
    "IMG_SIZE = 150\n",
    "BATCH_SIZE = 32\n",
    "\n",
    "train_dataset = image_dataset_from_directory(\n",
    "    train_dir,\n",
    "    image_size=(IMG_SIZE, IMG_SIZE),\n",
    "    batch_size=BATCH_SIZE,\n",
    "    label_mode='categorical'\n",
    ")\n",
    "\n",
    "validation_dataset = image_dataset_from_directory(\n",
    "    validation_dir,\n",
    "    image_size=(IMG_SIZE, IMG_SIZE),\n",
    "    batch_size=BATCH_SIZE,\n",
    "    label_mode='categorical'\n",
    ")\n",
    "\n",
    "test_dataset = image_dataset_from_directory(\n",
    "    test_dir,\n",
    "    image_size=(IMG_SIZE, IMG_SIZE),\n",
    "    batch_size=BATCH_SIZE,\n",
    "    label_mode='categorical'\n",
    ")\n",
    "\n",
    "AUTOTUNE = tf.data.AUTOTUNE\n",
    "train_dataset = train_dataset.prefetch(buffer_size=AUTOTUNE)\n",
    "validation_dataset = validation_dataset.prefetch(buffer_size=AUTOTUNE)\n",
    "\n",
    "def build_model(trial):\n",
    "    strategy = tf.distribute.MirroredStrategy()\n",
    "    print(\"GPUs usadas:\", strategy.num_replicas_in_sync)\n",
    "\n",
    "    with strategy.scope():\n",
    "        model = models.Sequential()\n",
    "        model.add(Input(shape=(IMG_SIZE, IMG_SIZE, 3)))\n",
    "\n",
    "        # Blocos Convolucionais\n",
    "        num_conv_blocks = trial.suggest_int(\"num_conv_blocks\", 1, 3)\n",
    "        for i in range(num_conv_blocks):\n",
    "            filters = trial.suggest_categorical(f\"filters_{i}\", [32, 64, 128])\n",
    "            model.add(layers.Conv2D(filters, (3, 3), activation='relu', padding=\"same\"))\n",
    "            model.add(layers.MaxPooling2D(2))\n",
    "\n",
    "        model.add(layers.Flatten())\n",
    "\n",
    "        # Dropout \n",
    "        use_dropout = trial.suggest_categorical(\"use_dropout\", [True, False])\n",
    "        if use_dropout:\n",
    "            dropout_rate = trial.suggest_float(\"dropout\", 0.3, 0.6)\n",
    "            model.add(layers.Dropout(dropout_rate))\n",
    "\n",
    "        # Dense + L2 \n",
    "        dense_units = trial.suggest_categorical(\"dense_units\", [128, 256, 512])\n",
    "        use_l2 = trial.suggest_categorical(\"use_l2\", [True, False])\n",
    "        if use_l2:\n",
    "            l2_val = trial.suggest_float(\"l2\", 1e-5, 1e-3, log=True)\n",
    "            reg = regularizers.l2(l2_val)\n",
    "        else:\n",
    "            reg = None\n",
    "        model.add(layers.Dense(dense_units, activation=\"relu\", kernel_regularizer=reg))\n",
    "\n",
    "        model.add(layers.Dense(7, activation=\"softmax\"))\n",
    "\n",
    "        lr = trial.suggest_float(\"lr\", 1e-4, 1e-2, log=True)\n",
    "        opt_name = trial.suggest_categorical(\"optimizer\", [\"adam\", \"rmsprop\", \"sgd\"])\n",
    "        if opt_name == \"adam\":\n",
    "            optimizer = optimizers.Adam(learning_rate=lr)\n",
    "        elif opt_name == \"rmsprop\":\n",
    "            optimizer = optimizers.RMSprop(learning_rate=lr)\n",
    "        else:\n",
    "            optimizer = optimizers.SGD(learning_rate=lr)\n",
    "\n",
    "        model.compile(optimizer=optimizer, loss=\"categorical_crossentropy\", metrics=[\"accuracy\"])\n",
    "\n",
    "    return model\n",
    "\n",
    "def objective(trial):\n",
    "    model = build_model(trial)\n",
    "    history = model.fit(\n",
    "        train_dataset,\n",
    "        validation_data=validation_dataset,\n",
    "        epochs=5,\n",
    "        verbose=0\n",
    "    )\n",
    "    return history.history[\"val_accuracy\"][-1]\n",
    "\n",
    "study = optuna.create_study(direction=\"maximize\")\n",
    "study.optimize(objective, n_trials=20, n_jobs=1)\n",
    "\n",
    "\n",
    "print(\"Melhores hiperparâmetros:\", study.best_params)\n"
   ]
  }
